fresh freshshell/fresh bin/fresh --bin

fresh bash_aliases --file
fresh bashrc --file
fresh colors --file
fresh git-ps1.sh --file
fresh gitconfig --file
fresh init.vim --file=~/.config/nvim/init.vim
fresh inputrc --file
fresh zshrc --file
fresh gitignore_global --file
fresh ctags --file
